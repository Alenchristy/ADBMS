mysql> INSERT INTO INVOICE_MASTER VALUES(2,102,'2021/09/12');
Query OK, 1 row affected (0.06 sec)

mysql> INSERT INTO INVOICE_MASTER VALUES(3,103,'2022/03/14');
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_MASTER VALUES(4,104,'2022/04/16');
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_MASTER VALUES(5,105,'2022/05/18');
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_MASTER VALUES(6,102,'2022/05/18');
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_MASTER VALUES(7,106,'2022/06/20');
Query OK, 1 row affected (0.05 sec)

mysql> SELECT * FROM INVOICE_MASTER;
