INSERTING VALUES INTO INVOICE ITEM TABLE->
mysql> INSERT INTO INVOICE_ITEM VALUES(1,10,50);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(2,21,20);
Query OK, 1 row affected (0.05 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(3,25,10);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(3,15,10);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(4,21,8);
Query OK, 1 row affected (0.07 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(5,18,105);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(6,10,75);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(6,18,60);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(7,15,43);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(7,25,21);
Query OK, 1 row affected (0.05 sec)

mysql> SELECT * FROM INVOICE_ITEM;
+--------+------+----------+
| INV_ID | PID  | QUANTITY |
